{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 300
      }
    }
  ],
  "routes": [
    {
      "src": "/api/python/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/*.py": {
      "memory": 3008,
      "maxDuration": 300
    }
  },
  "env": {
    "PYTHONPATH": "/var/task/api"
  }
}
